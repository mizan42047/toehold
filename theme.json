{
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"slug": "primary-000",
					"color": "#195EB1",
					"name": "Primary 000"
				},
				{
					"slug": "primary-100",
					"color": "#1B70D6",
					"name": "Primary 100"
				},
				{
					"slug": "primary-200",
					"color": "#2D8EFF",
					"name": "Primary 200"
				},
				{
					"slug": "primary-300",
					"color": "#4DA1FF",
					"name": "Primary 300"
				},
				{
					"slug": "primary-400",
					"color": "#8EC5FF",
					"name": "Primary 400"
				},
				{
					"slug": "primary-500",
					"color": "#D6EAFF",
					"name": "Primary 500"
				},
				{
					"slug": "secondary-000",
					"color": "#BC7108",
					"name": "Secondary 000"
				},
				{
					"slug": "secondary-100",
					"color": "#EE8B00",
					"name": "Secondary 100"
				},
				{
					"slug": "secondary-200",
					"color": "#FFA120",
					"name": "Secondary 200"
				},
				{
					"slug": "secondary-300",
					"color": "#FFB547",
					"name": "Secondary 300"
				},
				{
					"slug": "secondary-400",
					"color": "#FFCF88",
					"name": "Secondary 400"
				},
				{
					"slug": "secondary-500",
					"color": "#FFECCF",
					"name": "Secondary 500"
				},
				{
					"slug": "neutral-000",
					"color": "#000000",
					"name": "Neutral 000"
				},
				{
					"slug": "neutral-100",
					"color": "#6B717D",
					"name": "Neutral 100"
				},
				{
					"slug": "neutral-200",
					"color": "#9AA0AE",
					"name": "Neutral 200"
				},
				{
					"slug": "neutral-300",
					"color": "#BFC3CE",
					"name": "Neutral 300"
				},
				{
					"slug": "neutral-400",
					"color": "#E8EBF1",
					"name": "Neutral 400"
				},
				{
					"slug": "neutral-500",
					"color": "#FFFFFF",
					"name": "Neutral 500"
				}
			],
			"background": true,
			"custom": false,
			"defaultPalette": false,
			"customGradient": false,
			"defaultGradients": false
		},
		"layout": {
			"contentSize": "1100px",
			"wideSize": "1100px"
		},
		"spacing": {
			"units": [ "px", "em", "rem", "vh", "vw", "%" ],
			"spacingSizes": [
				{
					"name": "SM",
					"size": "8px",
					"slug": "sm"
				},
				{
					"name": "MD",
					"size": "16px",
					"slug": "md"
				},
				{
					"name": "LG",
					"size": "24px",
					"slug": "lg"
				},
				{
					"name": "XL",
					"size": "36px",
					"slug": "xl"
				}
			],
			"blockGap": true
		},
		"typography": {
			"dropCap": false,
			"fluid": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Figtree",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/Figtree/Figtree-Regular.ttf"
							]
						},
						{
							"fontFamily": "Figtree",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/Figtree/Figtree-Medium.ttf"
							]
						},
						{
							"fontFamily": "Figtree",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/Figtree/Figtree-SemiBold.ttf"
							]
						}
					],
					"fontFamily": "\"Figtree\", sans-serif",
					"name": "Figtree",
					"slug": "figtree"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Plus Jakarta Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/Plus_Jakarta_Sans/PlusJakartaSans-Regular.ttf"
							]
						},
						{
							"fontFamily": "Plus Jakarta Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/Plus_Jakarta_Sans/PlusJakartaSans-Bold.ttf"
							]
						}
					],
					"fontFamily": "\"Plus Jakarta Sans\", sans-serif",
					"name": "Plus Jakarta Sans",
					"slug": "plus-jakarta-sans"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "12px",
					"name": "Small",
					"fluid": false
				},
				{
					"slug": "medium",
					"size": "14px",
					"name": "Medium",
					"fluid": false
				},
				{
					"slug": "large",
					"size": "16px",
					"name": "Large",
					"fluid": false
				},
				{
					"slug": "extra-large",
					"size": "24px",
					"name": "Extra Large",
					"fluid": false
				},
				{
					"slug": "extra-extra-large",
					"size": "36px",
					"name": "Extra Extra Large",
					"fluid": false
				}
			],
			"customFontSize": true
		},
		"useRootPaddingAwareAlignments": true,
		"shadow": {
			"defaultPresets": true
		}
	},
	"styles": {
		"blocks": {
			"core/code": {
				"color": {
					"text": "var(--wp--preset--color--neutral-000)"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/latest-comments": {
				"spacing": {
					"padding": {
						"left": "0px"
					}
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--neutral-000)"
				}
			},
			"core/post-navigation-link": {
				"typography": {
					"fontWeight": "700"
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--neutral-200)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--neutral-200)"
						}
					}
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--neutral-200)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--neutral-200)"
						}
					}
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				},
				"color": {
					"text": "var(--wp--preset--color--neutral-200)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--neutral-000)"
						}
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-large)",
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)",
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--neutral-000)"
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)"
				},
				"color": {
					"text": "var(--wp--preset--color--neutral-000)"
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)",
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--neutral-000)"
				}
			},
			"core/query-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-extra-large)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/image": {
				"border": {
					"radius": "8px"
				}
			},
			"core/paragraph": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "400"
				},
				"color": {
					"text": "var(--wp--preset--color--neutral-000)"
				}
			},
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)",
					"fontSize": "var(--wp--preset--font-size--extra-large)",
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--neutral-000)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--neutral-500)",
			"text": "var(--wp--preset--color--neutral-000)"
		},
		"css": "",
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--neutral-000)"
				},
				"typography": {
					"textDecoration": "none"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--neutral-000)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--neutral-000)"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--neutral-000)"
					}
				}
			},
			"button": {
				"border": {
					"color": "var(--wp--preset--color--transparent)",
					"width": "4px",
					"style": "solid",
					"radius": "4px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--neutral-000)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "700"
				},
				"shadow": "var(--wp--preset--shadow--natural)",
				":hover": {
					"border": {
						"color": "var(--wp--preset--color--neutral-200)"
					}
				}
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)",
			"lineHeight": "1.5"
		},
		"spacing":{
			"blockGap": "40px",
			"padding": {},
			"margin": {}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "narrow-header",
			"title": "Narrow Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "narrow-footer",
			"title": "Narrow Footer",
			"area": "footer"
		},
		{
			"name": "sidebar",
			"title": "Sidebar",
			"area": "uncategorized"
		}
	],
	"customTemplates": [
		{
			"name": "single-product",
			"title": "WooCommerce single product (Requires WooCommerce)"
		},
		{
			"name": "archive-product",
			"title": "WooCommerce product archive (Requires WooCommerce)"
		}
	]
}
