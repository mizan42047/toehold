{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "toehold/card-image",
	"version": "1.0.0",
	"title": "Toehold Card image",
	"category": "toehold",
	"description": "Toehold card for toehold theme",
	"parent": ["toehold/card"],
	"example": {
		"attributes": {
			"preview": true
		}
	},
	"attributes": {
		"blockClass": {
			"type": "string"
		},
		"blockCSS": {
			"type": "string"
		},
		"badgeText": {
			"type": "string",
			"default": "Filling Fast"
		},
		"imageCaption": {
			"type": "string",
			"default": "Panther Paradise"
		},
		"badgeTextColor": {
			"type": "string",
			"default": "#fff"
		},
		"badgeBackgroundColor": {
			"type": "string",
			"default": "#ff6f61"
		},
		"badgeTextFontSize": {
			"type": "string",
			"default": "14px"
		},
		"badgePositionFromRight": {
			"type": "string"
		},
		"badgePositionFromTop": {
			"type": "string"
		},
		"captionFontSize": {
			"type": "string"
		},
		"captionFontWeight": {
			"type": "object",
			"default": {
				"fontStyle": "normal",
				"fontWeight": "400"
			}
		},
		"captionColor": {
			"type": "string"
		},
		"captionPositionFromBottom": {
			"type": "string"
		},
		"captionAlignment": {
			"type": "string"
		}

	},
	"supports": {
		"html": false
	},
	"textdomain": "toehold",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
